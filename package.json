{
  "name": "cmake-101",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@iconify-json/devicon": "^1.1.35",
    "@iconify-json/fluent-emoji": "^1.1.18",
    "@slidev/cli": "^0.48.1",
    "@slidev/theme-default": "latest",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "slides.md",
          "pages/*.md"
        ],
        "options": {
          "parser": "slidev",
          "plugins": [
            "prettier-plugin-slidev"
          ]
        }
      }
    ]
  }
}
